<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- App TextView -->
    <declare-styleable name="BTextView">
        <attr name="bFont" format="enum">
            <enum name="RobotoRegular" value="0" />
            <enum name="RobotoLight" value="1" />
            <enum name="RobotoBold" value="2" />
            <enum name="RobotoMedium" value="3" />
        </attr>
    </declare-styleable>

    <!-- App Toolbar -->
    <declare-styleable name="BToolbar">
        <attr name="tbTitle" format="string" />
    </declare-styleable>
</resources>